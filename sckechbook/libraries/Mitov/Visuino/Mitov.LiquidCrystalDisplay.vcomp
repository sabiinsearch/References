    [OWPrimaryPin]
    [ArduinoExclude]
    [ArduinoI2CInstanceTemplate]
    [ArduinoAddToTemplate( '0_IMPLEMENTATION' )]
    OutputPin : TOWArduinoI2CSourcePin

    [ArduinoAddToTemplate( '0_IMPLEMENTATION' )]
    Address : TArduinoUInt8 = $3E

  ;
