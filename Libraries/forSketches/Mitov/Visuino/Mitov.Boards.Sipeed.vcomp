  [ArduinoSerialType( 'UARTClass' )]
  +TArduinoSipeedK210Serial1 : TArduinoDefinedPinsBasicSpeedSerial

    [ValueRange( 0, 39 )]
    RXPin = 6

    [ValueRange( 0, 39 )]
    TXPin = 7

  ;
