    [ArduinoAddToTemplate( '0_IMPLEMENTATION' )]
    OutputPin : TOWArduinoSPISourcePin

    [ArduinoAddToTemplate( '0_IMPLEMENTATION' )]
    ChipSelectOutputPin : TOWArduinoDigitalFlexibleSourcePin

    [OWRequiredPin]
    [ArduinoAddToTemplate( '0_IMPLEMENTATION' )]
    DataCommandOutputPin : TOWArduinoDigitalFlexibleSourcePin

    Type : TArduinoDisplayOLEDType = odtSSD1306

  ;
