    [PropertyListDesignValue( '245' )]
    [PropertyListDesignValue( '500' )]
    [PropertyListDesignValue( '2000' )]
    [ArduinoUseBindingCheckSetter_Parent( 'Update_CTRL_REG1_G' )]
    [NamePostfix( '(dps)' )]
    FullScaleRange : Unsigned = 2000

