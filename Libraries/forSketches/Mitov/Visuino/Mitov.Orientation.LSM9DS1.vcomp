  [ArduinoClass( 'Mitov::TArduinoLSM9DS1Gyroscope' )]
  [ParentImage( TArduinoMPUXYZGyroscope )]
  TArduinoLSM9DS1Gyroscope : TArduinoBasicXYZSensor

    [ArduinoUseBindingCheckSetter_Parent( 'Update_CTRL_REG1_G' )]
