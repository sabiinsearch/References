    [PropertyListDesignValue( '4' )]
    [PropertyListDesignValue( '8' )]
    [PropertyListDesignValue( '16' )]
    [ArduinoUseBindingCheckSetter_Parent( 'Update_CTRL_REG6_XL' )]
