    [PropertyFixedListDesign]
    [PropertyListDesignValue( '14.9' )]
    [PropertyListDesignValue( '59.5' )]
    [PropertyListDesignValue( '119.0' )]
    [PropertyListDesignValue( '238.0' )]
    [PropertyListDesignValue( '476.0' )]
    [PropertyListDesignValue( '952.0' )]
