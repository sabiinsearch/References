  [Name( 'Buttons' )]
  [CreateName( 'Buttons' )]
  [ArduinoInclude( 'Mitov_RoMeo_BLE.h' )]
  [ArduinoClass( 'Mitov::ArduinoRoMeoBLEButtons' )]
  [ArduinoExcludeUnconnected]
  [ArduinoStart]
  [ArduinoLoopBegin]
  [ArduinoShieldPin( 'A7',  'Buttons' )]
  [ParentImage( TArduinoButton )]
  +TArduinoRoMeoBLEButtonsModule : TArduinoShield

    [OWPrimaryPin]
    [OWPinListPrimaryPinType( TOWArduinoDigitalFlexibleChangeOnlySourcePin )]
    [OWAddPin( 'S1', TOWArduinoDigitalFlexibleChangeOnlySourcePin )]
    [OWAddPin( 'S2', TOWArduinoDigitalFlexibleChangeOnlySourcePin )]
    [OWAddPin( 'S3', TOWArduinoDigitalFlexibleChangeOnlySourcePin )]
    [OWAddPin( 'S4', TOWArduinoDigitalFlexibleChangeOnlySourcePin )]
    [OWAddPin( 'S5', TOWArduinoDigitalFlexibleChangeOnlySourcePin )]
    OutputPins : TOWArduinoPinList

  ;
