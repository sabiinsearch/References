//---------------------------------------------------------------------------
  [Name( 'Raspberry Pi' )]
  [Category( TRaspberryPiARMCategory )]
  [ParentImage( TRaspberryPiCategory )]
  [ArduinoDefine( 'VISUINO_ARDUINO_RASPBERRY_PI' )]
  [ArduinoBoardArchitecture( 'piduino' )]
  [ArduinoBoardCompileParams( 'RaspberryPi:piduino:bplus' )]
  +TArduino_Raspberry_Pi_Board : TArduinoBoardExtendableImplementation

    [AddItem( TArduinoRaspberryPiConsoleModule )]
    [AddItem( TArduinoRaspberryPiNetworkModule )]
    Modules : TArduinoModules
    
    [AddItem( TArduinoSpeedOnlySerial )]
    Serial : TArduinoSerials

    [Name( 'I2C Channels' )]
    [AddItem( TArduinoI2C )]
    I2CChannels : TArduinoI2CChannels

    [Name( 'SPI Channels' )]
    [AddItem( TArduinoSPI )]
    SPIChannels : TArduinoSPIChannels

    [ArduinoBoard_Add_DigitalAnalogChannels( 2,  1, TArduinoAdditionalDigitalPullUpDownI2C0Channel, 'Digital(I2C-SDA)' )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 3,  1, TArduinoAdditionalDigitalPullUpDownI2C0Channel, 'Digital(I2C-SCL)' )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 4,  5, TArduinoAdditionalDigitalPullUpDownChannel )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 9,  1, TArduinoAdditionalDigitalPullUpDownSPI0Channel, 'Digital(SPI-MISO)' )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 10, 1, TArduinoAdditionalDigitalPullUpDownSPI0Channel, 'Digital(SPI-MOSI)' )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 11, 1, TArduinoAdditionalDigitalPullUpDownSPI0Channel, 'Digital(SPI-SCK)' )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 12, 2, TArduinoAdditionalDigitalPullUpDownPWMChannel )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 14, 1, TArduinoAdditionalDigitalPullUpDownSerial0Channel, 'Digital(TX)' )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 15, 1, TArduinoAdditionalDigitalPullUpDownSerial0Channel, 'Digital(RX)' )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 16, 2, TArduinoAdditionalDigitalPullUpDownChannel )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 18, 2, TArduinoAdditionalDigitalPullUpDownPWMChannel )]
    [ArduinoBoard_Add_DigitalAnalogChannels( 20, 8, TArduinoAdditionalDigitalPullUpDownChannel )]
    Digital : TArduinoDigitalChannels
    
    [ComponentEditorEntryDesign( 'Add &Hats ...' )]
    [Name( 'Hats' )]
    Shields : TArduinoRaspberryPiHats
    
  ;
